<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.sms"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="11"/>
    <application android:label="@string/app_name" android:icon="@drawable/ic_launcher">
        <activity android:name=".booking.BookingActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".confirmation.DisplayConfirmationActivity"
                android:label="@string/title_activity_display_confirmation"
                android:parentActivityName="com.example.sms.MyActivity"
                android:id="@+id/action_settings">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.sms.MyActivity" />
        </activity>
    </application>


</manifest>
